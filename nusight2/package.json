{
  "name": "NUsight",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "repository": "github:NUbots/NUsight2",
  "engines": {
    "node": ">=14.0.0",
    "yarn": "1.x"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "yarn test --ci --runInBand",
    "test:watch": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "coverage:ci": "yarn test:ci --coverage",
    "eslint": "eslint \"**/*.ts{,x}\" --cache",
    "eslint:fix": "eslint \"**/*.ts{,x}\" --cache --fix",
    "tscheck": "tsc --project tsconfig.test.json",
    "tscheck:watch": "yarn tsc --project tsconfig.test.json --watch",
    "protobuf": "node build_scripts/generate_messages.js",
    "postinstall": "patch-package && yarn protobuf",
    "dev": "yarn start:virtual",
    "dev:debug": "ts-node --inspect src/server/dev-server.ts --virtual-robots",
    "storybook": "node conf/storybook/dev.js",
    "storybook:build": "node conf/storybook/build_storybook.js",
    "storybook:prod": "node conf/storybook/prod.js",
    "start": "ts-node src/server/dev-server.ts",
    "start:debug": "ts-node --inspect src/server/dev-server.ts",
    "start:virtual": "ts-node src/server/dev-server.ts --virtual-robots",
    "virtual-robots": "nodemon src/virtual_robots/run.ts",
    "prod": "node dist/prod.js",
    "prod:virtual": "node dist/prod.js --virtual-robots",
    "build": "ts-node build_scripts/build.ts --production",
    "build:dev": "ts-node build_scripts/build.ts",
    "build:ci": "ts-node build_scripts/build.ts --ci",
    "clean": "yarn clean:dist && yarn clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "heroku-postbuild": "yarn build && yarn storybook:build"
  },
  "resolutions": {
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6"
  },
